{
  "name": "platform",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev -H localhost",
    "dev:builder": "pnpm dlx concurrently \"pnpm run tailwind\"  \"next dev\"",
    "build": "next build",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "tailwind": "pnpm dlx tailwindcss -i ../../packages/core/src/global.css -o ../../apps/platform/public/builder-tailwind.css --watch --config ../../packages/config/tailwindcss/tailwind.config.ts",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf .next"
  },
  "dependencies": {
    "@oe/api": "workspace:*",
    "@oe/assets": "workspace:*",
    "@oe/builder": "workspace:*",
    "@oe/core": "workspace:*",
    "@oe/dashboard": "workspace:*",
    "@oe/i18n": "workspace:*",
    "@oe/ui": "workspace:*",
    "@oe/themes": "workspace:*",
    "lucide-react": "^0.460.0",
    "next": "15.0.4-canary.20",
    "next-intl": "3.25.1",
    "react": "19.0.0-rc-6f0dc294-20241119",
    "react-dom": "19.0.0-rc-6f0dc294-20241119",
    "react-hook-form": "^7.53.2",
    "sonner": "^1.7.0",
    "swr": "^2.2.5",
    "zod": "^3.23.8",
    "zustand": "^5.0.1"
  },
  "devDependencies": {
    "@oe/config": "workspace:*",
    "@oe/types": "workspace:*",
    "@types/node": "^22.9.1",
    "@types/react": "npm:types-react@rc",
    "@types/react-dom": "npm:types-react-dom@rc",
    "babel-plugin-react-compiler": "19.0.0-beta-0dec889-20241115",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.15",
    "typescript": "^5.6.3"
  },
  "packageManager": "pnpm@9.14.2"
}

{
  "name": "@oe/themes",
  "version": "0.0.1",
  "private": true,
  "imports": {
    "#types": "./src/types.ts"
  },
  "exports": {
    "./*": "./src/*.tsx",
    ".": "./src/index.ts",
    "./package.json": "./package.json",
    "./store/*": "./src/_store/*.ts",
    "./config/*": "./src/_config/*.ts",
    "./components/*": "./src/_components/*.tsx",
    "./common/*": "./src/_common/*.tsx",
    "./types": "./src/_types/index.ts",
    "./utils/*": "./src/_utils/*.ts"
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "peerDependencies": {
    "@oe/api": "workspace:*",
    "@oe/assets": "workspace:*",
    "@oe/ui": "workspace:*",
    "cookies-next": "^5.1.0",
    "next": "^15.2.4",
    "next-intl": "4.0.2",
    "react": "19.1.0",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@oe/config": "workspace:*",
    "@oe/core": "workspace:*",
    "@oe/types": "workspace:*",
    "@types/node": "^22.14.0",
    "@types/react": "19.1.0",
    "@types/react-dom": "19.1.1",
    "@types/wicg-file-system-access": "^2023.10.6",
    "typescript": "^5.8.2"
  }
}

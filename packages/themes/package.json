{
  "name": "@oe/themes",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "sideEffects": false,
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "files": ["dist"],
  "imports": {
    "#components/*": "./src/_components/*.tsx",
    "#common/*": "./src/_common/*.tsx",
    "#config/*": "./src/_config/*.ts",
    "#store/*": "./src/_store/*.ts",
    "#types": "./src/_types/index.ts",
    "#utils/*": "./src/_utils/*.ts"
  },
  "exports": {
    ".": {
      "development": "./src/index.ts",
      "import": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/cjs/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsx ../../scripts/build/main.ts --dts --clean",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "clean:build": "rm -rf .turbo && rm -rf dist"
  },
  "dependencies": {
    "@oe/api": "workspace:*",
    "@oe/assets": "workspace:*",
    "@oe/core": "workspace:*",
    "@oe/ui": "workspace:*",
    "cookies-next": "^5.1.0",
    "date-fns": "^4.1.0",
    "lucide-react": "0.487.0",
    "next": "15.3.1-canary.4",
    "next-intl": "4.0.2",
    "react": "19.1.0",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@oe/config": "workspace:*",
    "@oe/types": "workspace:*",
    "@types/node": "^22.14.0",
    "@types/react": "19.1.0",
    "@types/react-dom": "19.1.2",
    "typescript": "^5.8.3"
  }
}

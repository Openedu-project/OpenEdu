{
  "name": "@oe/assets",
  "private": true,
  "version": "0.3.0",
  "description": "",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "development": "./src/index.ts",
      "import": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/cjs/index.cjs"
      }
    },
    "./images/*": {
      "development": "./src/images/*",
      "import": "./dist/esm/images/*",
      "require": "./dist/cjs/images/*"
    }
  },
  "type": "module",
  "sideEffects": false,
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "files": ["dist"],
  "scripts": {
    "build": "tsx ../../scripts/build/main.ts --dts --clean",
    "typecheck": "tsc",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "clean:build": "rm -rf .turbo && rm -rf dist"
  },
  "devDependencies": {
    "@oe/config": "workspace:*",
    "@oe/types": "workspace:*",
    "@types/node": "^22.15.17"
  }
}

{
  "name": "@oe/dashboard",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "sideEffects": false,
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "development": "./src/index.ts",
      "import": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/cjs/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsx ../../scripts/build/main.ts --dts --clean",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "clean:build": "rm -rf .turbo && rm -rf dist"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.0.1",
    "@oe/api": "workspace:*",
    "@oe/assets": "workspace:*",
    "@oe/core": "workspace:*",
    "@oe/i18n": "workspace:*",
    "@oe/ui": "workspace:*",
    "lucide-react": "0.510.0",
    "next": "15.4.0-canary.34",
    "next-intl": "4.1.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.56.3",
    "recharts": "^2.15.3",
    "swr": "^2.3.3",
    "use-debounce": "^10.0.4"
  },
  "devDependencies": {
    "@oe/config": "workspace:*",
    "@oe/types": "workspace:*",
    "@types/node": "^22.15.18"
  }
}
